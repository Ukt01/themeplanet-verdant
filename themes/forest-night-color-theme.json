    "button.background": "#6b8e6b",
    "button.foreground": "#1a2b1f",
    "button.hoverBackground": "#4a7c59"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7E9285", "fontStyle": "italic" }
    },
    {
      "name": "Keywords / Control",
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#78AEEA" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#A9B8AE" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string", "string.template", "string.quoted"],
      "settings": { "foreground": "#7CCB8A" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#E3C26A" }
    },
    {
      "name": "Constants (true/false/null/undefined)",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": { "foreground": "#E3C26A" }
    },
    {
      "name": "Functions / Methods",
      "scope": ["entity.name.function", "support.function", "meta.function-call", "variable.function"],
      "settings": { "foreground": "#4FB7A6" }
    },
    {
      "name": "Types / Classes / Interfaces",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": { "foreground": "#E3C26A" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": { "foreground": "#DCE7DE" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#CFEFD6" }
    },
    {
      "name": "Properties / Members",
      "scope": ["variable.other.property", "variable.other.member"],
      "settings": { "foreground": "#D2E6FF" }
    },

    {
      "name": "JSX/HTML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": { "foreground": "#78AEEA" }
    },
    {
      "name": "JSX/HTML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#4FB7A6" }
    },

    {
      "name": "Markdown Headings",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#E3C26A", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#78AEEA", "fontStyle": "underline" }
    },
    {
      "name": "Markdown Inline/Block Code",
      "scope": ["markup.raw.inline", "markup.raw.block"],
      "settings": { "foreground": "#CFEFD6", "background": "#15231B" }
    }
  ],
  "semanticHighlighting": true
}
