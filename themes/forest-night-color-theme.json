    "terminal.ansiBlue": "#4a7c59",
    "terminal.ansiMagenta": "#8fa88f",
    "terminal.ansiCyan": "#2d5a47",
    "terminal.ansiWhite": "#e8f5e8",
    "terminal.ansiBrightBlack": "#5a7a5a",
